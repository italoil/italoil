<div class="page-header">
  <div class="container">
    <h1>{{ page.title | default: 'Wholesale Program' }}</h1>
    <p class="page-header__subtitle">
      {{ section.settings.subtitle | default: 'Premium Italian Olive Oil for Restaurants, Retailers & Food Services' }}
    </p>
  </div>
</div>

{% schema %}
{
  "name": "Wholesale Page Header",
  "tag": "section",
  "settings": [
    {
      "type": "textarea",
      "id": "subtitle",
      "label": "Subtitle",
      "default": "Premium Italian Olive Oil for Restaurants, Retailers & Food Services"
    }
  ]
}
{% endschema %}
